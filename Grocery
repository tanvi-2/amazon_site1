    String n = Price1.getText(); 
     g1.setText(c+"");
     g2.setText(n);
     
     String b = name2.getText();
    String p = Price2.getText(); 
     g3.setText(b+"");
     g4.setText(p);
     
     String d = name3.getText();
    String q = Price3.getText(); 
     g5.setText(d+"");
     g6.setText(q);  
     
     String e = name4.getText();
    String t = Price4.getText(); 
     g7.setText(e+"");
     g8.setText(t);
        




// TODO add your handling code here:
    }                                        

